<!doctype html>
	<div>
	<a href="javascript:history.back()" class="link"><h3><- BACK</h3></a>
	</div>
	<div class="proj_title">
		<h1>{{project}}</h1>
		<div class="proj_details">
			{{#each overview}}
			{{#each this}}
			<h5>{{this}}</h5>
			{{/each}}
			{{/each}}
		</div>
	</div>
	<main>
	{{#each category}}
	<div class ="paragraph">
		{{#if Group}}
		<h4>{{Group}}</h4>
		{{/if}}
	<div class="row">
		<div class="col-lg-5">	
			<h3> {{Title}}</h3>
		</div>
		<div class="col-lg-7">
			<p>{{Paragraph}}</p>
		</div>
		{{#if image}}
		<div class="background full_photo">
			<img src="./public/images/{{image}}" alt="{{altText}}">
		</div>
		{{/if}}
	</div>
</div>
	{{/each}}
</main>