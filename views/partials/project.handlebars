<!doctype html>
	<div>
	<a href="javascript:history.back()" class="link"><h3><- BACK</h3></a>
	</div>
	<div class="proj_title">
		<h1>{{project}}</h1>
		<div class="proj_details">
			{{#each overview}}
			{{#each this}}
			<h5>{{this}}</h5>
			{{/each}}
			{{/each}}
		</div>
		
	</div>
	<div class = "background full_photo">
			<img src="./public/images/{{image}}" alt="{{altText}}">
		</div>
	<main>
	{{#each category}}
	{{#if type}}
	<div class = "splash">
		<div>
			<h4>{{Title}}</h4>
			<h2>{{Paragraph}}</h2>
		</div>
	</div>
	{{else}}
	<div class ="paragraph">
		{{#if Group}}
		<h4>{{Group}}</h4>
		{{/if}}
		<div class="row">
			<div class="col-lg-5">	
				<h4> {{Title}}</h4>
				<div class="icon">
				<img src="./public/icons/{{icon}}" alt="" ></img>
			</div>
			</div>
			<div class="col-lg-7">
				<p class = "bold">{{Subtitle}}</p>
				<p>{{Paragraph}}</p>
			</div>
		</div>
	</div>
	{{#if image}}
	<div class = "background full_photo">
		<h3>{{caption}}</h3>
		<img src="./public/images/{{image}}" alt="{{altText}}">
	</div>
	{{/if}}

	{{/if}}
	{{/each}}
</main>