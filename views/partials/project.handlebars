<!doctype html>
	<div ="proj_details">
		<div>
			<h3>{{Title}}</h3>
			<h4>{{Subtitle}}</h4>
			<p>{{disclaimer}}</p>
		</div>
		<div class="full_photo">
			<img src="./public/images/{{sectionSplashImg}}">
		</div>
		{{#each category}}
		<div>
			<h4>{{Title}}</h4>
			<div>
				<div>
					{{#each details}}
					<p>{{this}} </p>							
					{{/each}}
				</div>
			</div>
		</div>
		{{/each}}
	</div>
