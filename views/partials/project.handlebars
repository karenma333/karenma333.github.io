<!doctype html>
	<div>
	<a href="javascript:history.back()" class="link"><h3><- BACK</h3></a>
	</div>
	<article>
	<div class="proj_title">
		<header>
			<h1 role= "heading" aria-level="1">{{project}}</h1>
		</header>
		<div class="proj_details">
			{{#each overview}}
			{{#each this}}
			<h5>{{this}}</h5>
			{{/each}}
			{{/each}}
		</div>
		
	</div>
	<div class = "background full_photo">
			<img src="./public/images/{{image}}" alt="{{altText}}">
		</div>
	{{#each category}}
	{{#if type}}
	<div class = "splash">
		<div>
			<h4 role="heading" aria-level="2">{{Title}}</h4>
			<h2>{{Paragraph}}</h2>
		</div>
	</div>
	{{else}}
	<div class ="paragraph">
		{{#if Group}}
		<h4>{{Group}}</h4>
		{{/if}}
		<div class="row">
			<div class="col-lg-5">	
				<h4 role="heading" aria-level="2">  {{Title}}</h4>
				<div class="icon">
				<img src="./public/icons/{{icon}}" alt=""  aria-hidden="true"></img>
			</div>
			</div>
			<div class="col-lg-7">
				<p class = "bold" role="heading" aria-level="3">{{Subtitle}}</p>
				<p>{{Paragraph}}</p>
			</div>
		</div>
	</div>
	{{#if image}}
	<div class = "background full_photo row">
		<img src="./public/images/{{image}}" alt="{{altText}}">
		<div class = "col-lg-12">
			<p>{{caption}}</p>
		 </div>
	</div>
	{{/if}}

	{{/if}}
	{{/each}}
</article>