<!doctype html>
	<article>
		<div class="row projTitle">
			<div class="col-lg-2">
				<div class="backButton">
					<a href="javascript:history.back()" > <img src="./public/icons/backArrow.png"><h3> BACK</h3></a>
				</div>
			</div>
			<div class= "proj_intro col-lg-10">
				<div class="sectionTitle">
					<h1 role= "heading" aria-level="1">{{project}}</h1>
				</div>
			</div>
		</div>
		<div class="row section">
			<div class="sectionTitle">
				<h2> Context </h2>
			</div>
			<div class="sectionBody">
				<header class = "secondary">
					<div class="proj_details">
						{{#each overview}}
						{{#each this}}
						<h5>{{this}}</h5>
						{{/each}}
						{{/each}}
					</div>
				</header>
				<div class="disclaimer secondary">
					<p >{{disclaimer}}</p>
				</div>
				<div class = "full_photo">
					<img src="./public/images/{{image}}" alt="{{altText}}">
				</div>
			</div>
		</div>
	<div class="row writeUp">
		{{#each category}}
			<div class="section">
			{{#if type}}
				<div class = "sectionTitle splash">
					<h3 role="heading" aria-level="2">{{Title}}</h3>
				</div>
				{{/if}}

				<div class="sectionBody">
					<div class ="subsection">
						<h4 role="heading" aria-level="2">  {{Title}}</h4>
						<p>{{Paragraph}}</p>
					</div>
					{{#if image}}
					<div class = "background full_photo row">
						<img src="./public/images/{{image}}" alt="{{altText}}">
						<div>
							<p>{{caption}}</p>
						 </div>
					</div>
					{{/if}}
				</div>
			</div>
		{{/each}}
	</div>
</article>